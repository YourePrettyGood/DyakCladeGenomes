mkdir -p logs
mkdir -p BUSCO_summaries
Running BUSCO v3 on Dyak_NY73PB
/usr/bin/time -v run_BUSCO.py -i ../refs/Dyak_NY73PB.fasta -l /home/pfreilly/Downloads/Bioinformatics/Assembly/busco/diptera_odb9/ -m genome -c 8 --long -z -o Dyak_NY73PB_BUSCOv3_genome_diptera 2> logs/Dyak_NY73PB_BUSCOv3_genome_diptera.stderr > logs/Dyak_NY73PB_BUSCOv3_genome_diptera.stdout; \
tail -n10 logs/Dyak_NY73PB_BUSCOv3_genome_diptera.stdout | head -n7 | awk 'BEGIN{printf "%s", "Dyak_NY73PB";}NR>1{printf "\t%s", $2;}END{print "";}' > Dyak_NY73PB_buscos.tsv; \
cp run_Dyak_NY73PB_BUSCOv3_genome_diptera/short_summary_Dyak_NY73PB_BUSCOv3_genome_diptera.txt BUSCO_summaries/short_summary_Dyak_NY73PB.txt
